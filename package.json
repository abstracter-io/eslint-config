{
  "name": "@abstracter/eslint-config",
  "version": "0.0.0",
  "private": false,
  "description": "common eslint configuration for common env/stacks",
  "scripts": {
    "commit": "git-cz",
    "build": "node scripts/build.js",
    "release": "node scripts/release.js",
    "lint": "eslint --format pretty --ext=.js,.ts --fix ."
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Elad Chen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abstracter-io/eslint-config"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@abstracter/atomic-release": "^1.2.0",
    "eslint": "^8.24.0",
    "eslint-formatter-pretty": "^4.1.0",
    "git-cz": "^4.9.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^13.17.0"
  }
}
