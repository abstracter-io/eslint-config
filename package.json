{
  "name": "@abstracter/eslint-config",
  "version": "0.0.0",
  "private": false,
  "description": "Common eslint configuration for common env/stacks",
  "files": [
    "lib/**"
  ],
  "main": "lib/index.js",
  "scripts": {
    "outdated": "npx npm-check -u",
    "lint": "eslint --format pretty --fix",
    "lint:all": "npm run lint -- .",
    "prepare": "husky",
    "release": "node scripts/release.mjs"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Elad Chen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abstracter-io/eslint-config.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@abstracter/atomic-release": "^2.2.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-formatter-pretty": "^6.0.1",
    "git-cz": "^4.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "micromatch": "^4.0.7"
  },
  "dependencies": {
    "@eslint/js": "^9.8.0",
    "@stylistic/eslint-plugin": "^2.6.2",
    "eslint-formatter-pretty": "^6.0.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-promise": "^7.1.0",
    "typescript-eslint": "^8.0.1"
  },
  "peerDependencies": {
    "eslint": "^9.8.0"
  }
}
